	<text>
		<body xml:space="preserve">
			<div type="dedication">
				<p xml:id="p1">[Madrigali del med.˚ Tassino]</p>
			</div>
			<div>
				<lg>
					<l xml:id="l1">Caro amoroso Neo</l>
					<l xml:id="l2">Che si illustri un bel volto</l>
					<l xml:id="l3">Col nero tuo nel suo candor accolto</l>
				</lg>
				<lg>
					<l xml:id="l4">Se per te stesso sei</l>
					<l xml:id="l5">Tu pur macchia e difetto</l>
					<l xml:id="l6">Con quale arte perfetto</l>
					<l xml:id="l7">Poi rendi il colmo delle gratie in lei?</l>
				</lg>
				<lg>
					<l xml:id="l8">Forse macchie si belle</l>
					<l xml:id="l9">Sono del ciel le stelle</l>
					<l xml:id="l10">Ma se tali hà costei</l>
					<l xml:id="l11">In sua belta le mende</l>
					<l xml:id="l12">Qual poi saranno i fregi ond’ella splende?</l>
				</lg>
			</div>
		</body>
	</text>
