	<text>
		<body xml:space="preserve">
			<div type="dedication">
				<p xml:id="p1">A 6.</p>
			</div>
			<div>
				<lg>
					<l xml:id="l1">Ha Ninfe adorn'e belle</l>
					<l xml:id="l2">La casta Margarita &amp; essa è Dea</l>
					<l xml:id="l3">Se virtù fà gli Dei come solea.</l>
				</lg>
				<lg>
					<l xml:id="l4">Però boschi palaggi e prati e valli</l>
					<l xml:id="l5">Secchi et ondosi calli</l>
					<l xml:id="l6">Le fece il grande Alfonso E cinse intorno</l>
					<l xml:id="l7">Navi E d'erranti fere ampio soggiorno</l>
					<l xml:id="l8">E gionse i porti e i lustri in cui le serra</l>
					<l xml:id="l9">Perche sia la prigion campo di guerra</l>
					<l xml:id="l10">E i diletti sian glorie</l>
					<l xml:id="l11">Et tante prede sue tutte vittorie.</l>
				</lg>
			</div>
		</body>
	</text>
