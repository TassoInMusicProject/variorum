					<l xml:id="l1">[TACET]</l>
					<l xml:id="l2">Qual pescator le reti</l>
					<l xml:id="l3">Frà cari scogli [TACET]</l>
					<l xml:id="l4">Et un bel fior vedea</l>
					<l xml:id="l5">Ch’à tutti i fior più lieti</l>
					<l xml:id="l6">Facea nel'onde amare un dolce scorno</l>
					<l xml:id="l7">E pien di meraviglia</l>
					<l xml:id="l8">Dicea, felice preda e chi la piglia.</l>
