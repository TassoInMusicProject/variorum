			<div>
				<lg>
					<l xml:id="l1">Al lume de le stelle,</l>
					<l xml:id="l2">Tirsi sotto un alloro,</l>
					<l xml:id="l3">Si dolea lagrimando in questi accenti.</l>
				</lg>
				<lg>
					<l xml:id="l4">O celesti facelle,</l>
					<l xml:id="l5">Di lei châ€™amo, &amp; adoro</l>
					<l xml:id="l6">Rassomigliate voi gli occhi lucenti</l>
					<l xml:id="l7"><seg xml:id="l7v1">[OMITTED]</seg></l>
					<l xml:id="l8"><seg xml:id="l8v1">[OMITTED]</seg></l>
					<l xml:id="l9">Luci serene, e liete</l>
					<l xml:id="l10">Sento <seg xml:id="l10v1">la fiamma</seg> lor mentre splendete.</l>
				</lg>
			</div>
