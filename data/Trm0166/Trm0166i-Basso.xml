				<lg>
					<l xml:id="l1">[TACET]</l>
					<l xml:id="l2">Se parlo acresco l’ira</l>
					<l xml:id="l3">Donna bell'e crudel che mi martira</l>
					<l xml:id="l4">Ma pur prendo speranza</l>
					<l xml:id="l5">Che l’humiltà vi pieghi</l>
					<l xml:id="l6">Che nel silentio ancor son voci e prieghi</l>
					<l xml:id="l7">E preg'amor che spieghi</l>
					<l xml:id="l8">Nel mio doglioso aspetto</l>
					<l xml:id="l9">Con lettre di pietà l’occulto affetto.</l>
				</lg>
