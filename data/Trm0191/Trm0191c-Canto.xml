					<l xml:id="l1">Arsi mentre a voi piacque</l>
					<l xml:id="l2">Et al cortese affetto</l>
					<l xml:id="l3">Pag√≤ tributo di sospir il petto</l>
					<l xml:id="l4">Ma poi ch'il vostro Amore</l>
					<l xml:id="l5">Volgeste in altra parte</l>
					<l xml:id="l6">Donna s'estinse anco il mio foc'in parte</l>
					<l xml:id="l7">Sorse poi novo ardore</l>
					<l xml:id="l8">Ma fiamma fu di sdegn'e non d'Amore.</l>
