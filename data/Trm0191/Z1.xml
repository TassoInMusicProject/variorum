	<text>
		<body xml:space="preserve">
			<div type="dedication">
				<p xml:id="p1">Madrigale</p>
			</div>
			<div>
				<lg>
					<l xml:id="l1">Arsi mentre à voi piacque</l>
					<l xml:id="l2">Et al cortese affetto</l>
					<l xml:id="l3">Pagò tributo di sospiri il petto</l>
					<l xml:id="l4">Ma poi ch'el bel disio</l>
					<l xml:id="l5">Volgeste in altra parte</l>
					<l xml:id="l6">Donna s'estinse anco'l mio foco in parte.</l>
					<l xml:id="l7">Sorse poi novo ardore,</l>
					<l xml:id="l8">Ma fu fiamma di sdegno</l>
					<l xml:id="l9">Ch’el mio dono di voi stimaste indegno.</l>
					<l xml:id="l10">[OMITTED]</l>
					<l xml:id="l11">[OMITTED]</l>
				</lg>
