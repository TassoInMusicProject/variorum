					<l xml:id="l19">Picciola e l’Ape e fa col picciol morso</l>
					<l xml:id="l20">Pur grav’e pur moleste le ferite</l>
					<l xml:id="l21">Ma qual cosa è più picciola d’Amore,</l>
					<l xml:id="l22">Se’ in ogni breve spatio entra e s’asconde</l>
					<l xml:id="l23">In ogni breve spatio [TACET]</l>
					<l xml:id="l24">[TACET] hor tra minuti rivi</l>
					<l xml:id="l25">d’un biondo crine [TACET]</l>
					<l xml:id="l26">[TACET]</l>
					<l xml:id="l27">E pur fa tanto grandi e si mortali</l>
					<l xml:id="l28">E così immedicabile le piaghe</l>
					<l xml:id="l29">Ohime che tutt’è piag’e tutto sangue</l>
					<l xml:id="l30">Son le viscere mie</l>
					<l xml:id="l31">[TACET]</l>
					<l xml:id="l32">Crudel Amore</l>
					<l xml:id="l33">[TACET] O com’a te confassi</l>
					<l xml:id="l34">Tal nome e quanto vide chi tel’ pose</l>
					<l xml:id="l35">Celan le selv’Angui Leon’&amp; Orsi</l>
					<l xml:id="l36">dent’il lor verde [TACET]</l>
					<l xml:id="l37">fere peggior ch’angui Leon’&amp; Orsi</l>
					<l xml:id="l38">[TACET] questi placarsi</l>
					<l xml:id="l39">Non possono per prego ne per dono</l>
					<l xml:id="l40">[TACET]</l>
					<l xml:id="l41">Ohime quand’io ti porgo i vaghi pomi</l>
					<l xml:id="l42">Tu li rifiuti disdegnosa Forse</l>
					<l xml:id="l43">perche pomi più vaghi hai nel bel seno</l>
					<l xml:id="l44">[TACET]</l>
					<l xml:id="l45">Ma se mia povertà non puo donarti</l>
					<l xml:id="l46">Cosa ch’in te più bell’e dolce non sia</l>
					<l xml:id="l47">[TACET] Hor pche iniqua</l>
					<l xml:id="l48">schern’e abhor’il dono</l>
					<l xml:id="l49">[OMITTED]</l>
					<l xml:id="l50">[OMITTED]</l>
					<l xml:id="l51">[OMITTED]</l>
					<l xml:id="l52">[OMITTED]</l>
					<l xml:id="l53">[OMITTED]</l>
					<l xml:id="l54">[OMITTED]</l>
					<l xml:id="l55">[OMITTED]</l>
					<l xml:id="l56">[OMITTED]</l>
					<l xml:id="l57">[OMITTED]</l>
					<l xml:id="l58">[OMITTED]</l>
					<l xml:id="l59">[OMITTED]</l>
					<l xml:id="l60">[OMITTED]</l>
					<l xml:id="l61">[OMITTED]</l>
					<l xml:id="l62">[OMITTED]</l>
					<l xml:id="l63">[OMITTED]</l>
					<l xml:id="l64">[OMITTED]</l>
					<l xml:id="l65">[OMITTED]</l>
					<l xml:id="l66">Non son io brutto no ne tu mi sprezzi</l>
					<l xml:id="l67">perche si fatt’io sia Ma solamente</l>
					<l xml:id="l68">perche povero sono Hai che le ville</l>
					<l xml:id="l69">Seguon l’essempio de le gran Cittadi</l>
					<l xml:id="l70">E veramente il secol d’oro e questo</l>
					<l xml:id="l71">poi che sol vince l’oro e regna l’oro</l>
					<l xml:id="l72">[TACET]</l>
					<l xml:id="l73">[TACET]</l>
					<l xml:id="l74">[OMITTED]</l>
					<l xml:id="l75">[OMITTED]</l>
					<l xml:id="l76">[OMITTED]</l>
					<l xml:id="l77">[OMITTED]</l>
					<l xml:id="l78">[OMITTED]</l>
					<l xml:id="l79">[OMITTED]</l>
					<l xml:id="l80">[OMITTED] Tu prima svergognasti</l>
					<l xml:id="l81">La nobilità d’Amore [TACET]</l>
					<l xml:id="l82">Amor venale</l>
					<l xml:id="l83">Amor servo de l’or’e’l maggior mostro</l>
					<l xml:id="l84">[TACET]</l>
					<l xml:id="l85">Che produca la terr’e’l mar fra l’onde</l>
					<l xml:id="l86">Ma perch’in van mi lagno Usa ciascun</l>
					<l xml:id="l87">quell’armi che gli ha data la natura</l>
					<l xml:id="l88">per sua salute [TACET]</l>
					<l xml:id="l89">il Leon gli Artigli [TACET]</l>
					<l xml:id="l90">E son potenz’&amp; armi</l>
					<l xml:id="l91">De la donna bellezza e leggiadria</l>
					<l xml:id="l92">Io perche non per mia salute adopro</l>
					<l xml:id="l93">La violenza se mi fe natura</l>
					<l xml:id="l94">[TACET]</l>
					<l xml:id="l95">Pianga sospiri pur usi ogni sforzo</l>
					<l xml:id="l96">Di pietà di bellezza [TACET]</l>
					<l xml:id="l97">Indi non partirà ch’io pria non tinga</l>
					<l xml:id="l98">L’arme mie per vendetta nel suo sangue.</l>
				</lg>
