					<l xml:id="l14">[EXTRA]</l>
					<l xml:id="l15">Picciol’e l’Ap’e fa col picciol morso</l>
					<l xml:id="l16">Pur grav’e pur moleste le ferite</l>
					<l xml:id="l17">Ma qual cosa e più picciola d’Amore,</l>
					<l xml:id="l18">Se in ogni breve spatio entra e s’ascond’</l>
					<l xml:id="l19">in ogni breve spatio hor sott’a l’ombra</l>
					<l xml:id="l20">de le palpebre [TACET]</l>
					<l xml:id="l21">[TACET] hor dentro le pozzette</l>
					<l xml:id="l22">Che form’un dolce riso in bella guancia</l>
					<l xml:id="l23">E pur fa tanto grandi e si mortali</l>
					<l xml:id="l24">E così immedicabile le piaghe</l>
					<l xml:id="l25">[TACET]</l>
					<l xml:id="l26">[TACET] e mille spiedi</l>
					<l xml:id="l27">Ha ne gl’occhi di Silvia’l crud’amore.</l>
					<l xml:id="l28">[TACET] Silvia crudel’ed empia</l>
					<l xml:id="l29">Più che le selve O com’a te confassi</l>
					<l xml:id="l30">Tal nome [TACET]</l>
					<l xml:id="l31">[TACET]</l>
					<l xml:id="l32">[TACET] e tu dentr’al bel petto</l>
					<l xml:id="l33">Nascondi odio disdegn’&amp; impietate</l>
					<l xml:id="l34">Fere peggior ch’angui Leon’&amp; Orsi</l>
					<l xml:id="l35">Che si placano quei questi placarsi</l>
					<l xml:id="l36">Non possono per prego ne per dono</l>
					<l xml:id="l37">Ohime quando ti porto i fior novelli</l>
					<l xml:id="l38">Tu li riccusi ritrosetta Forse</l>
					<l xml:id="l39">perche fior via piu bell’hai nel bel volto</l>
					<l xml:id="l40">[TACET]</l>
					<l xml:id="l41">[TACET]</l>
					<l xml:id="l42">[TACET]</l>
					<l xml:id="l43">Ohime quand’io t’offrisco il dolce mele</l>
					<l xml:id="l44">tu lo disprezzi dispettosa Forse</l>
					<l xml:id="l45">perche me’l via più dolc’hai ne le labra</l>
					<l xml:id="l46">Ma se mia povertà non puo donarti</l>
					<l xml:id="l47">Cosa ch’in te più bel’e dolce non sia</l>
					<l xml:id="l48">Me medesmo ti dono  [TACET]</l>
					<l xml:id="l49">[TACET]</l>
					<l xml:id="l50">[OMITTED]</l>
					<l xml:id="l51">[OMITTED]</l>
					<l xml:id="l52">[OMITTED]</l>
