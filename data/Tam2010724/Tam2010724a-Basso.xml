					<l xml:id="l20">Picciol’e l’Ape e fa col picciol morso</l>
					<l xml:id="l21">Pur grav’e pur moleste le ferite</l>
					<l xml:id="l22">[TACET]</l>
					<l xml:id="l23">[TACET] hor sott’a l’ombra</l>
					<l xml:id="l24">de le palpebre [TACET]</l>
					<l xml:id="l25">[TACET]</l>
					<l xml:id="l26">hor dentr’a le pozzette</l>
					<l xml:id="l27">Che form’un dolce riso in bella guancia</l>
					<l xml:id="l28">E pur fa tanto grandi e si mortali</l>
					<l xml:id="l29">E così immedicabile le piaghe</l>
					<l xml:id="l30">[TACET]</l>
					<l xml:id="l31">e mille spiedi</l>
					<l xml:id="l32">Ha ne gl’occhi di Silvia’l crud’amore.</l>
					<l xml:id="l33">[TACET] Silvia crudel’ed empia</l>
					<l xml:id="l34">Più che le selve O com’a te confassi</l>
					<l xml:id="l35">tal nome e quanto vide chi tel’ pose</l>
					<l xml:id="l36">[TACET]</l>
					<l xml:id="l37">e tu dentr’al bel petto</l>
					<l xml:id="l38">Nascondi odio disdegn’&amp; impietate</l>
					<l xml:id="l39">Fere peggior ch’angui Leon’&amp; Orsi</l>
					<l xml:id="l40">Che si placano quei questi placarsi</l>
					<l xml:id="l41">Non possono per prego ne per dono</l>
					<l xml:id="l42">Ohime quando ti porto i fior novelli</l>
					<l xml:id="l43">Tu li riccusi ritrosetta Forse</l>
					<l xml:id="l44">perche fior via piu bell’hai nel bel volto</l>
					<l xml:id="l45">[TACET]</l>
					<l xml:id="l46">Ohime quand’io t’offrisco il dolce mele</l>
					<l xml:id="l47">tu lo disprezzi dispettosa Forse</l>
					<l xml:id="l48">pche me’l via più dolc’hai ne le labra</l>
					<l xml:id="l49">Ma se mia povertà non puo donarti</l>
					<l xml:id="l50">Cosa ch’in te più bell’e dolce non sia</l>
					<l xml:id="l51">Me medesmo ti dono  [TACET]</l>
					<l xml:id="l52">[TACET]</l>
					<l xml:id="l53">[OMITTED]</l>
					<l xml:id="l54">[OMITTED]</l>
					<l xml:id="l55">[OMITTED]</l>
					<l xml:id="l56">[OMITTED]</l>
					<l xml:id="l57">[OMITTED]</l>
