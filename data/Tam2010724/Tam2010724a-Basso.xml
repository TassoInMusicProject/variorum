					<l xml:id="l68">[OMITTED]</l>
					<l xml:id="l69">[OMITTED]</l>
					<l xml:id="l70">hai che le ville</l>
					<l xml:id="l71">Seguon l’essempio de le gran Cittadi</l>
					<l xml:id="l72">e veramente il secol d’oro e questo</l>
					<l xml:id="l73">poi che sol vince l’oro e regna l’oro</l>
					<l xml:id="l74">O chiunque tu fosti ch’insegnasti</l>
					<l xml:id="l75">prim’a vender l’amor sia maledetto</l>
					<l xml:id="l76">[OMITTED]</l>
					<l xml:id="l77">[OMITTED]</l>
					<l xml:id="l78">[OMITTED]</l>
					<l xml:id="l79">[OMITTED]</l>
					<l xml:id="l80">[OMITTED]</l>
					<l xml:id="l81">[OMITTED]</l>
					<l xml:id="l82">[TACET]</l>
					<l xml:id="l83">[TACET] tu le sue liete</l>
					<l xml:id="l84">dolcezz’inamaristi [TACET]</l>
					<l xml:id="l85">Et e il più abominabil e’l più sozzo</l>
					<l xml:id="l86">Che produca la terr’e’l mar fra l’onde.</l>
					<l xml:id="l87">[TACET]</l>
					<l xml:id="l88">Il Cerv’adopra il corso</l>
					<l xml:id="l89">[TACET] &amp; il bavoso</l>
					<l xml:id="l90">Cinghial il dente e son potenz’&amp; armi</l>
					<l xml:id="l91">De la donna bellezza e leggiadria</l>
					<l xml:id="l92">[TACET]</l>
					<l xml:id="l93">Sforzerò rapirò quel che costei</l>
					<l xml:id="l94">Mi nieg’ingrat’in merto de l’Amore</l>
					<l xml:id="l95">Pianga sospiri pur usi ogni sforzo</l>
					<l xml:id="l96">di pietà di bellezza che s’io posso</l>
					<l xml:id="l97">Questa mano ravolgerle nel crine</l>
					<l xml:id="l98">Indi non partirà ch’io pria non tinga</l>
					<l xml:id="l99">L’arme mie per vendetta nel suo sangue.</l>
				</lg>
