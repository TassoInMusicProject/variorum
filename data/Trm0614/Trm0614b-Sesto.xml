				<lg>
					<l xml:id="l1">Al vostro dolce azuro</l>
					<l xml:id="l2">Cedan luci serene</l>
					<l xml:id="l3">Quai piu neri Italia in preggio tiene</l>
					<l xml:id="l4">[OMITTED]</l>
					<l xml:id="l5">[OMITTED]</l>
					<l xml:id="l6">[TACET]</l>
					<l xml:id="l7">Azuro e’l ciel eterno</l>
					<l xml:id="l8">E cio c’ha bello il bel l’ha sol da lui</l>
					<l xml:id="l9">E bell’e sol perche somigl’a vui.</l>
				</lg>
