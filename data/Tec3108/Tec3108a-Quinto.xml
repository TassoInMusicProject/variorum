			<div>
				<lg>
					<l xml:id="l1">Non sospirar Pastor non lagrimare</l>
					<l xml:id="l2">Queste lagrime amare</l>
					<l xml:id="l3">Che spargi da tuoi lumi</l>
					<l xml:id="l4">Non spegneran scintilla de l'ardore</l>
					<l xml:id="l5">Ov'ogn'hor ti consumi</l>
					<l xml:id="l6">Che s’Amor dalle fiamme del tuo core</l>
					<l xml:id="l7">Puo trarre humor'e venti</l>
					<l xml:id="l8">Trarrà dal piant'ancor faville ardenti.</l>
				</lg>
			</div>
