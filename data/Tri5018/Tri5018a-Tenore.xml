				<lg>
					<l xml:id="l1">Tu perfido Signor Tu disleale</l>
					<l xml:id="l2">[TACET]</l>
					<l xml:id="l3">Oggetto desti impar’e disseguale</l>
					<l xml:id="l4">Ond’a pieno m’affliga al mio pensiero</l>
					<l xml:id="l5">Deh mie stelle crudeli hor quando tale</l>
					<l xml:id="l6">Scempio fu visto cosi strano e fiero</l>
					<l xml:id="l7">Che dove in altri Amor da speme nasce</l>
					<l xml:id="l8">Dal non sperar in me s’avviva e pasce.</l>
				</lg>
