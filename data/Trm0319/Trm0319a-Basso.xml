			<div type="dedication">
				<p xml:id="p1">Seconda parte.</p>
			</div>
			<div>
				<lg>
					<l xml:id="l1">Non puo l’humil terreno</l>
					<l xml:id="l2">Tra primi Abeti e faggi</l>
					<l xml:id="l3">[TACET]</l>
					<l xml:id="l4">E'l vivo e dolce foco</l>
					<l xml:id="l5">E chi nasconde il Sole</l>
					<l xml:id="l6">Perche non splenda pur com’egli suole</l>
					<l xml:id="l7">Occhi soavi e cari</l>
					<l xml:id="l8">Occhi sereni e chiari</l>
					<l xml:id="l9">Voi somigliar sovente</l>
					<l xml:id="l10">Fate l'Angusta villa a l'Oriente.</l>
				</lg>
			</div>
