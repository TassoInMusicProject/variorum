			<div>
				<lg>
					<l xml:id="l1">Io vidi già sotto l’ardente sole</l>
					<l xml:id="l2">Discolorati i fiori</l>
					<l xml:id="l3">Come la mia Licori,</l>
				</lg>
				<lg>
					<l xml:id="l4">Come i gigli che'l volto, e le viole</l>
					<l xml:id="l5">Che d’irrigar desio</l>
					<l xml:id="l6">Con lagrimoso rio,</l>
				</lg>
				<lg>
					<l xml:id="l7">E seco insiem’impalidire anch’io</l>
					<l xml:id="l8">Seco mutar sembiante:</l>
					<l xml:id="l9">[ABSENT]</l>
				</lg>
			</div>
