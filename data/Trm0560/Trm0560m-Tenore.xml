			<div>
				<lg>
					<l xml:id="l1">Mentre mia stella miri</l>
					<l xml:id="l2">I bei stellati giri</l>
					<l xml:id="l3">Il ciel esser vorrei</l>
					<l xml:id="l4">Perche ne lumi miei</l>
					<l xml:id="l5">Fisso tu rivolgessi</l>
					<l xml:id="l6">le tue dolci faville</l>
					<l xml:id="l7">Io vagheggiar potessi</l>
					<l xml:id="l8">Mille bellezze tue con occhi mille.</l>
				</lg>
			</div>
