				<lg>
					<l xml:id="l1"><seg xml:id="l1v1">Mentre mia stella miri</seg></l>
					<l xml:id="l2"><seg xml:id="l2v1">I bei stellati giri</seg></l>
					<l xml:id="l3"><seg xml:id="l3v1">Il ciel esser vorrei</seg></l>
					<l xml:id="l4"><seg xml:id="l4v1">Perche ne lumi miei</seg></l>
					<l xml:id="l5"><seg xml:id="l5v1">Fisso tu rivolgessi</seg></l>
					<l xml:id="l6"><seg xml:id="l6v1">le tue dolci faville</seg></l>
					<l xml:id="l7"><seg xml:id="l7v1">Io vagheggiar potessi</seg></l>
					<l xml:id="l8"><seg xml:id="l8v1">Mille bellezze tue con occhi mille.</seg></l>
