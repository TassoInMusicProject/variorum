				<lg>
					<l xml:id="l1">Lungi da voi mio core</l>
					<l xml:id="l2">[ABSENT]</l>
					<l xml:id="l3">mille volte m’uccid'il mio dolore</l>
					<l xml:id="l4">per che la mia partita</l>
					<l xml:id="l5">mi tolse l’alma e s'io ripenso in lei</l>
					<l xml:id="l6">mi ritoglie la vita</l>
					<l xml:id="l7">[ABSENT]</l>
					<l xml:id="l8">ò miseria infinita</l>
					<l xml:id="l9">felice e quello ch'una volta more.</l>
				</lg>
