			<div>
				<lg>
					<l xml:id="l1">Quest'è pur quella mano</l>
					<l xml:id="l2">Che con si belle tempre</l>
					<l xml:id="l3">Mi trasse il cor dal petto</l>
				</lg>
				<lg>
					<l xml:id="l4">Deh perche m'e disdetto</l>
					<l xml:id="l5">Hor che l'ho già cosi [illegible]</l>
					<l xml:id="l6">Con la mia man ristretta</l>
					<l xml:id="l7">[OMITTED]</l>
					<l xml:id="l8">[OMITTED]</l>
					<l xml:id="l9">[OMITTED]</l>
					<l xml:id="l10">Farne con mille bacci alta vendeta</l>
					<l xml:id="l11">[OMITTER]</l>
				</lg>
