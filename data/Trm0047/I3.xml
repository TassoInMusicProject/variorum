					<l xml:id="l1"><seg xml:id="l1v1">Quest'è pur quella</seg> mano</l>
					<l xml:id="l2">Che <seg xml:id="l2v1">con si belle tempre</seg></l>
					<l xml:id="l3"><seg xml:id="l3v1">Mi trasse il</seg> cor <seg xml:id="l3v2">dal petto</seg></l>
				</lg>
				<lg>
					<l xml:id="l4"><seg xml:id="l4v1">Deh perche m'e disdetto</seg></l>
					<l xml:id="l5"><seg xml:id="l5v1">Hor che l'ho già cosi [illegible]</seg></l>
					<l xml:id="l6"><seg xml:id="l6v1">Con la mia man ristretta</seg></l>
					<l xml:id="l7"><seg xml:id="l7v1">[OMITTED]</seg></l>
					<l xml:id="l8"><seg xml:id="l8v1">[OMITTED]</seg></l>
					<l xml:id="l9"><seg xml:id="l9v1">[OMITTED]</seg></l>
					<l xml:id="l10"><seg xml:id="l10v1">Farne con mille bacci alta vendeta</seg></l>
