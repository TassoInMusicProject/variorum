			<div>
				<lg>
					<l xml:id="l1">Non si levav'ancor l’alba novella</l>
					<l xml:id="l2">Ne spiegavan le piume</l>
					<l xml:id="l3">Gl'augelli al novo lume</l>
					<l xml:id="l4">Ma fiammeggiava l’amorosa stella.</l>
				</lg>
				<lg>
					<l xml:id="l5">Quando due vagh'e leggiadrett'amanti</l>
					<l xml:id="l6">[OMITTED]</l>
					<l xml:id="l7">[OMITTED]</l>
					<l xml:id="l8">[OMITTED]</l>
					<l xml:id="l9">[OMITTED]</l>
					<l xml:id="l10">[OMITTED]</l>
					<l xml:id="l11">[OMITTED]</l>
					<l xml:id="l12">[OMITTED]</l>
					<l xml:id="l13">[OMITTED]</l>
					<l xml:id="l14">[OMITTED]</l>
					<l xml:id="l15">[TACET] sospirand'all'hora</l>
					<l xml:id="l16">Anima à Dio Con languide parole</l>
					<l xml:id="l17">E l’altra vita à Dio li rispondea</l>
					<l xml:id="l18">A Dio rimanti e non partiansi ancora</l>
					<l xml:id="l19">[OMITTED]</l>
					<l xml:id="l20">[OMITTED]</l>
					<l xml:id="l21">E quest'e quella impallidir vedea</l>
					<l xml:id="l22">Le bellissime rose</l>
					<l xml:id="l23">Nelle labr'amorose</l>
					<l xml:id="l24">E gl'occhi scintillar come facella.</l>
					<l xml:id="l25">[OMITTED]</l>
					<l xml:id="l26">[OMITTED]</l>
					<l xml:id="l27">[OMITTED]</l>
