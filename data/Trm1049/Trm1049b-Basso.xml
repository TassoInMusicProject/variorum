					<l xml:id="l1"><seg xml:id="l1v1">[TACET]</seg> E <seg xml:id="l1v2">questo</seg> amore</l>
					<l xml:id="l2">Comincia in novi modi</l>
					<l xml:id="l3">Queste note son nodi</l>
					<l xml:id="l4">Che mi fa la <seg xml:id="l4v1">mia Donna</seg> intorn’al core</l>
